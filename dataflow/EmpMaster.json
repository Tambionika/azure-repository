{
	"name": "EmpMaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmpMaster",
						"type": "DatasetReference"
					},
					"name": "EmpMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SQL_EMP_MD",
						"type": "DatasetReference"
					},
					"name": "sinkSQLempMD"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {100001} as string,",
				"          AACHEN as string,",
				"          DAVID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> EmpMaster",
				"EmpMaster sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          EmployeeID = {100001},",
				"          LastName = AACHEN,",
				"          FirstName = DAVID",
				"     )) ~> sinkSQLempMD"
			]
		}
	}
}