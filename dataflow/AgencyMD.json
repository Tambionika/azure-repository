{
	"name": "AgencyMD",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AgencyMaster",
						"type": "DatasetReference"
					},
					"name": "AgencyMD"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SQL_Agency_MD",
						"type": "DatasetReference"
					},
					"name": "sinkSQLAgencyMaster"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AgencyMD",
				"AgencyMD sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          AgencyID = Column_1,",
				"          AgencyName = Column_2",
				"     )) ~> sinkSQLAgencyMaster"
			]
		}
	}
}